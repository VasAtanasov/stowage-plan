<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui">
  <ribbon>
    <tabs>
      <tab id="CargoPlanTab" label="Cargo Plan">
        <group id="CargoPlanGroup" label="Calculations">
          <button id="UnitsAndWeightsButton" label="Units and Weights" size="large" onAction="CargoPlan.CalculateUnitsAndWeights" imageMso="Calculator" />
          <button id="UnloadPortsButton" label="Clear Ports" size="large" onAction="CargoPlan.UnloadSelectedPorts" imageMso="AddInsMenu" />
          <button id="PreArrivalButton" label="Pre-Arrival" size="large" onAction="CargoPlan.PreArrivalPort" imageMso="DataSubmitToOfficeServer" />
        </group>
        <group id="DrawingPlanGroup" label="Drawings">
          <button id="AddCargoTextBox" label="Cargo Text Box" size="large" onAction="CargoBox.AddCargoTextBox" imageMso="TextBoxWordClassic" />
        </group>
        <group id="PortsArrangementGroup" label="Ports Arrangement">
          <button id="MoveRowUpButton" label="Move Row Up" size="normal" onAction="TableArrangement.MoveTableRowUp" imageMso="ShapeUpArrow" />
          <button id="MoveRowDownButton" label="Move Row Down" size="normal" onAction="TableArrangement.MoveTableRowDown" imageMso="ShapeDownArrow" />
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI> 